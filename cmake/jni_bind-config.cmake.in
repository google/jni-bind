@PACKAGE_INIT@

include("${CMAKE_CURRENT_LIST_DIR}/jni_bind-targets.cmake")
check_required_components(jni_bind)

# Try to find JNI if needed
if(NOT JNI_FOUND)
  find_package(JNI QUIET)
  if(JNI_FOUND)
    message(STATUS "JNI found: ${JNI_INCLUDE_DIRS}")
  else()
    message(STATUS "JNI not found. You may need to link JNI manually.")
  endif()
endif()